  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "apollo-server": "^3.7.0",
    "graphql": "^16.5.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "prisma": "^3.14.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5",
    "yarn": "^1.22.18"
  }
}
